pkgname="hurlin"
pkgver="0.1.1"
pkgrel="1"
pkgdesc="A hurl runner with an HTTP library to import and export from native hurl"
arch=("x86_64")
license=("GPL-3.0-or-later")
source=("hurlin")
sha512sums=("SKIP")


package() {
  mkdir -p "${pkgdir}/usr/bin"
  cp "${srcdir}/hurlin" "${pkgdir}/usr/bin/hurlin"
  chmod +x "${pkgdir}/usr/bin/hurlin"
}
